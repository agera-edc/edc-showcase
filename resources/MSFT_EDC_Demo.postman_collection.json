		{
			"name": "Request Data (async)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let responseData=pm.response.text();",
							"let json = JSON.parse(responseData);",
							"pm.environment.set(\"processId\", json.response)"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"id\": \"412123341234\",\n    \"edctype\": \"dataspaceconnector:datarequest\",\n    \"connectorAddress\":\"{{provider_url}}\",\n    \"protocol\": \"ids-rest\",\n    \"connectorId\":\"consumer\",\n    \"asset\":{\n        \"id\": \"test-document-az_provider\",\n        \"policyId\": \"use-us\"\n    },\n    \"dataDestination\":{\n        \"type\": \"AmazonS3\"\n    },\n    \"managedResources\": true,\n    \"transferType\":{\n        \"contentType\": \"application/octet-stream\",\n        \"isFinite\": true\n    }\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{consumer_url}}/api/datarequest",
					"host": [
						"{{consumer_url}}"
					],
					"path": [
						"api",
						"datarequest"
					]
				}
			},
			"response": []
		},
